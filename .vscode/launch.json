  "version": "0.2.0",
  "configurations": [
    {
      "name": "banking",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}\\banking",
      "env": {},
      "args": [
        "-AUTH_ADDRESS=localhost:8001",
        "-SERVER_ADDRESS=localhost",
        "-SERVER_PORT=8000",
        "-DB_USER=root",
        "-DB_PASSWORD=my-secret-pw",
        "-DB_HOST=localhost",
        "-DB_NAME=banking",
        "-DB_PORT=3306"
       ]
    },
    {
      "name": "banking-auth",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}\\banking-auth",
      "env": {},
      "args": [
        "-SERVER_ADDRESS=localhost",
        "-SERVER_PORT=8001",
        "-DB_USER=root",
        "-DB_PASSWORD=my-secret-pw",
        "-DB_HOST=localhost",
        "-DB_NAME=banking",
        "-DB_PORT=3306"
       ]
    }
  ]
}